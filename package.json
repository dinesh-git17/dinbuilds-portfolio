		"react-dom": "19.2.3",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"tailwindcss": "^4",
		"typescript": "^5"
